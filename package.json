        "babel-loader": "^8.2.2",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
        "copyfiles": "^2.4.1",
        "css-loader": "^5.2.4",
        "css-minimizer-webpack-plugin": "^2.0.0",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "gh-pages": "^3.2.3",
        "jest": "^26.6.3",
        "mini-css-extract-plugin": "^1.6.0",
        "onchange": "^7.1.0",
        "prettier": "^2.3.0",
        "react-test-renderer": "^17.0.2",
        "rollup": "^2.58.0",

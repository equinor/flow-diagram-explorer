{
    "name": "@equinor/flow-diagram-explorer",
    "description": "Explorer for flow diagrams",
    "repository": {
        "type": "git",
        "url": "https://github.com/equinor/flow-diagram-explorer"
    },
    "bugs": {
        "url": "https://github.com/equinor/flow-diagram-explorer/issues"
    },
    "homepage": "https://equinor.github.io/flow-diagram-explorer",
    "license": "MPL-2.0",
    "author": {
        "name": "Equinor",
        "email": "noreply@equinor.com"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.6.0"
    },
    "main": "dist/equinor_flow_diagram_explorer.cjs.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist/*"
    ],
    "publishConfig": {
        "main": "dist/equinor_flow_diagram_explorer.cjs.js",
        "module": "dist/equinor_flow_diagram_explorer.esm.js",
        "browser": "dist/equinor_flow_diagram_explorer.umd.js"
    },
    "dependencies": {
        "@date-io/dayjs": "^2.17.0",
        "@equinor/eds-core-react": "^0.33.1",
        "@equinor/eds-icons": "^0.19.3",
        "clsx": "^2.0.0",
        "dagre": "^0.8.5",
        "dayjs": "^1.11.10",
        "react": "^18.2.0",
        "react-content-loader": "^6.2.1",
        "react-dom": "^18.2.0",
        "styled-components": "^6.1.1"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "styled-components": "^6.1.1"
    },
    "scripts": {
        "predeploy": "npm run build-storybook",
        "deploy": "gh-pages -d storybook-static",
        "dev": "rollup -c -w NODE_ENV=development",
        "build": "rollup -c",
        "prepack": "npm run build",
        "prepublishOnly": "npm run lint",
        "preversion": "npm run lint",
        "test": "jest",
        "typecheck": "tsc --noEmit",
        "format": "eslint --fix 'src/**/*.+(ts|tsx|json|css|js|jsx)'",
        "lint": "eslint 'src/**/*.+(ts|tsx|json|js|jsx)'",
        "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
        "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest",
            "plugin:storybook/recommended"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "browser": {
        "child_process": false,
        "fs": false
    },
    "devDependencies": {
        "@babel/preset-env": "^7.23.2",
        "@babel/preset-react": "^7.22.15",
        "@babel/preset-typescript": "^7.23.2",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-replace": "^5.0.5",
        "@storybook/addon-essentials": "^7.5.3",
        "@storybook/addon-links": "^7.5.3",
        "@storybook/react": "^7.5.3",
        "@storybook/react-webpack5": "^7.5.3",
        "@svgr/rollup": "^8.1.0",
        "@svgr/webpack": "^8.1.0",
        "@types/dagre": "^0.7.52",
        "@types/jest": "^29.5.8",
        "@types/node": "^20.9.0",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.1.3",
        "babel-plugin-styled-components": "^2.1.4",
        "eslint": "^8.53.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-storybook": "^0.6.15",
        "gh-pages": "^6.0.0",
        "jest": "^29.7.0",
        "onchange": "^7.1.0",
        "prettier": "^3.0.3",
        "rollup": "^4.3.0",
        "rollup-plugin-delete": "^2.0.0",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-typescript-paths": "^1.4.0",
        "rollup-plugin-typescript2": "^0.36.0",
        "storybook": "^7.5.3",
        "typescript": "^5.2.2"
    }
}
